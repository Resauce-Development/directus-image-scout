{
  "name": "@resauce-dev/directus-image-scout",
  "version": "3.0.0",
  "description": "Image Scout is a Directus 9 interface that allowing you to search online image libraries for images.",
  "main": "index.js",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-interface",
    "extension",
    "image",
    "library",
    "vue",
    "javascript",
    "unsplash",
    "pexels",
    "pixabay",
    "giphy",
    "royalty free",
    "resauce"
  ],
  "author": "Shea Lavington <shealavington@gmail.com>",
  "license": "GNU GENERAL PUBLIC LICENSE",
  "scripts": {
    "build-endpoints": "directus-extension build -t endpoint -i ./src/endpoints/index.ts -o ./dist/endpoints/resauce-image-scout/index.js --sourcemap",
    "build-interface": "directus-extension build -t interface -i ./src/interface/index.ts -o ./dist/interfaces/resauce-image-scout/index.js --sourcemap",
    "dev-endpoints": "directus-extension build -t endpoint -i ./src/endpoints/index.ts -o ../directus-resauce/extensions/endpoints/resauce-image-scout/index.js -w --no-minify",
    "dev-interface": "directus-extension build -t interface -i ./src/interface/index.ts -o ../directus-resauce/extensions/interfaces/resauce-image-scout/index.js -w --no-minify",
    "build": "npm run build-endpoints && npm run build-interface",
    "dev": "npm run dev-endpoints && npm run dev-interface",
    "dev_old": "directus-extension build -w --no-minify"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "9.18.1",
    "@rollup/plugin-typescript": "^8.5.0",
    "rollup-plugin-copy": "^3.4.0",
    "typescript": "^4.8.4",
    "vue": "^3.2.40"
  },
  "directus:extension": {
    "xxtype": "interface",
    "xxpath_dev": "../directus-resauce/extensions/interfaces/resauce-image-scout/index.js",
    "xxpath": "dist/interfaces/resauce-image-scout/index.js",
    "xxsource": "src/interface/index.ts",
    "host": "^9.18.1"
  }
}
