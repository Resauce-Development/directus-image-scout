{
  "name": "@resauce-dev/directus-image-scout",
  "version": "1.1.0",
  "description": "Image Scout is a Directus 9 interface that allowing you to search online image libraries for images.",
  "main": "index.js",
  "keywords": [
    "directus", "extension",
    "royalty free", "image", "library",
    "unsplash", "pexels", "pixabay", "giphy",
    "vue", "javascript",
    "resauce"
  ],
  "author": "Shea Lavington <shea@lavington.me>",
  "license": "GNU GENERAL PUBLIC LICENSE",
  "scripts": {
    "build": "npx rollup -c",
    "watch": "npm-watch build"
  },
  "watch": {
    "build": {
      "patterns": ["src"],
      "extensions": "js,vue"
    }
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.0.0-rc.11",
    "rollup": "^2.26.11",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.12",
    "npm-watch": "^0.7.0"
  }
}
